{
  "name": "frontend-starter-pack",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@lingui/macro": "^2.9.2",
    "@lingui/react": "^2.9.2",
    "antd": "^4.8.0",
    "antd-theme": "^0.3.4",
    "axios": "^0.19.2",
    "mobx": "^5.15.7",
    "mobx-react": "^6.3.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-hook-form": "^4.10.2",
    "react-router-dom": "^5.2.0",
    "socket.io-client": "^2.3.1",
    "styled-components": "^5.2.1",
    "tailwindcss": "^1.9.6"
  },
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "prestart": "npm run build:css && npm run i18n:compile",
    "prebuild": "npm run build:css:prod && npm run i18n:compile",
    "build:css": "postcss tailwind.css -o src/tailwind-generated.css",
    "build:css:prod": "postcss tailwind.css -o src/tailwind-generated.css --env production",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "test": "react-app-rewired test",
    "i18n:add": "lingui add-locale",
    "i18n:extract": "lingui extract",
    "i18n:compile": "lingui compile",
    "eject": "react-app-rewired eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md,html}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@fullhuman/postcss-purgecss": "^2.3.0",
    "@lingui/cli": "^2.9.2",
    "@testing-library/jest-dom": "^5.11.5",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^8.1.3",
    "@types/jest": "^25.2.3",
    "@types/lingui__core": "^2.7.1",
    "@types/lingui__macro": "^2.7.4",
    "@types/lingui__react": "^2.8.3",
    "@types/node": "^13.13.30",
    "@types/react": "^16.9.55",
    "@types/react-dom": "^16.9.9",
    "@types/react-router-dom": "^5.1.6",
    "@types/socket.io-client": "^1.4.34",
    "@types/styled-components": "^5.1.4",
    "autoprefixer": "^9.8.6",
    "babel-core": "^7.0.0-bridge.0",
    "babel-plugin-import": "^1.13.1",
    "customize-cra": "^0.9.1",
    "cypress": "^4.12.1",
    "husky": "^4.3.0",
    "less": "^3.12.2",
    "less-loader": "^5.0.0",
    "lint-staged": "^10.5.1",
    "postcss-cli": "^7.1.2",
    "prettier": "^1.19.1",
    "react-app-rewired": "^2.1.6",
    "react-scripts": "3.3.1",
    "source-map-explorer": "^2.5.0",
    "start-server-and-test": "^1.11.5",
    "styless": "0.0.3",
    "typescript": "^3.9.7"
  }
}
